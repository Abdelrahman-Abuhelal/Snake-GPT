{
  "manifest_version": 3,
  "name": "Browser Content Extension",
  "version": "1.0",
  "permissions": ["tabs"],
  "background": {
    "js": ["background.js"]
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js":["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {

  }
}
