{
  "manifest_version": 3,
  "name": "Browser Snake Extension",
  "version": "1.0",
  "permissions":
   ["tabs",
  "storage",
  "activeTab",
  "scripting"],
  "background": {
    "service_worker": "background.mjs",
     "type": "module" 
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "mjs":["api.mjs"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {}
}
